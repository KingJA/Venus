<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--对话框样式-->
    <style name="CustomAlertDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/shape_lnull_bwhite_r8</item>
    </style>

    <style name="font_blue_16">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="font_3_16">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/font_3</item>
    </style>

    <style name="font_9_15">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/font_9</item>
    </style>

    <!--普通垂直分割线-->
    <style name="divider_nomal_vertical">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/gray_divider</item>
    </style>
    <!--普通水平分割线-->
    <style name="divider_nomal_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/gray_divider</item>
    </style>
</resources>